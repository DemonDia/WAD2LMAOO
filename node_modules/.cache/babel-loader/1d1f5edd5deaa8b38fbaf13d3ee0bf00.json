GAAiB,KAAjB;AACJ;AACJ;AAVK,GAPE;AAmBT,EAAA,WAnBS,yBAmBI,CACf;AACA;AACA;AACA;AACA;AACD,GAzBY;AA0BX,EAAA,IA1BW,kBA0BJ;AACH,WAAO;AACH,MAAA,YAAY,EAAE,EADX;AAEH,MAAA,UAAU,EAAE,EAFT;AAGH,MAAA,YAAY,EAAE,EAHX;AAIH,MAAA,MAAM,EAAE,EAJL;AAKH,MAAA,IAAI,EAAE,EALH;AAMH,MAAA,SAAS,EAAE,KANR;AAOH,MAAA,OAAO,EAAE,CAAC,MAAD,EAAQ,MAAR,EAAe,QAAf,EAAwB,QAAxB,CAPN,CAQH;AACA;AACA;AACA;AACA;AACA;;AAbG,KAAP;AAeH,GA1CU;AA2CX,EAAA,QAAQ,EAAE;AACN,IAAA,YADM,0BACS;AAAA;;AACP,aAAO,KAAK,UAAL,CAAgB,MAAhB,CAAuB,UAAA,GAAE,EAAK;AACrC,YAAM,IAAG,GAAI,GAAG,CAAC,SAAJ,CAAc,WAAd,EAAb;AACA,YAAM,MAAK,GAAI,GAAG,CAAC,WAAJ,CAAgB,WAAhB,EAAf;AACA,YAAM,OAAM,GAAI,GAAG,CAAC,YAAJ,CAAiB,WAAjB,EAAhB;;AACA,YAAM,UAAS,GAAI,KAAI,CAAC,MAAL,CAAY,WAAZ,EAAnB;;AAEA,eAAO,IAAI,CAAC,QAAL,CAAc,UAAd,KAA6B,MAAM,CAAC,QAAP,CAAgB,UAAhB,CAA7B,IAA4D,OAAO,CAAC,QAAR,CAAiB,UAAjB,CAAnE;AACH,OAPU,CAAP;AAQP,KAVK;AAWN,IAAA,cAXM,4BAWU;AACZ,UAAI,KAAI,GAAI,CAAZ;AACA,UAAI,aAAY,GAAI,KAAK,YAAzB;;AACA,WAAI,IAAI,CAAA,GAAI,CAAZ,EAAe,CAAA,GAAI,aAAa,CAAC,MAAjC,EAAyC,CAAC,EAA1C,EAA6C;AACzC;AACA,QAAA,KAAK,IAAG,aAAa,CAAC,CAAD,CAAb,CAAiB,MAAzB;AACJ;;AAEA,aAAO,KAAK,GAAC,aAAa,CAAC,MAA3B;AACJ;AApBM,GA3CC;AAiEX,EAAA,OAjEW,qBAiED;AAAA;;AACN,IAAA,QAAQ,CAAC,IAAT,GAAgB,kBAAhB,CAAmC,UAAC,IAAD,EAAU;AACzC,UAAI,IAAJ,EAAU;AACN,QAAA,QAAQ,CAAC,QAAT,GAAoB,GAApB,CAAwB,WAAW,IAAI,CAAC,GAAxC,EAA8C,EAA9C,CAAiD,OAAjD,EAA0D,UAAC,QAAD,EAAc;AACpE,UAAA,MAAI,CAAC,YAAL,CAAkB,IAAlB,CAAuB,QAAQ,CAAC,GAAT,EAAvB;;AACA,UAAA,OAAO,CAAC,GAAR,CAAY,MAAI,CAAC,YAAjB;AACH,SAHD;AAIA,QAAA,QAAQ,CAAC,QAAT,GAAoB,GAApB,CAAwB,QAAxB,EAAkC,EAAlC,CAAqC,OAArC,EAA8C,UAAC,QAAD,EAAc;AACxD,UAAA,QAAQ,CAAC,OAAT,CAAiB,UAAC,aAAD,EAAmB;AAChC,gBAAI,IAAG,GAAI,aAAa,CAAC,GAAd,EAAX;;AACA,gBAAI,IAAI,CAAC,OAAL,KAAiB,IAAI,CAAC,GAA1B,EAA+B;AAC3B,cAAA,OAAO,CAAC,GAAR,CAAY,IAAZ;;AACA,cAAA,MAAI,CAAC,UAAL,CAAgB,IAAhB,CAAqB,IAArB;AACJ;AACH,WAND;AAOH,SARD;AASA,QAAA,QAAQ,CAAC,QAAT,GAAoB,GAApB,CAAwB,UAAxB,EAAoC,EAApC,CAAuC,OAAvC,EAAgD,UAAC,QAAD,EAAc;AAC1D,UAAA,MAAI,CAAC,YAAL,GAAoB,EAApB;AACA,UAAA,QAAQ,CAAC,OAAT,CAAiB,UAAC,aAAD,EAAmB;AAChC,gBAAI,MAAK,GAAI,aAAa,CAAC,GAAd,EAAb;;AACA,gBAAI,MAAM,CAAC,OAAP,KAAmB,IAAI,CAAC,GAA5B,EAAiC;AAC7B,cAAA,OAAO,CAAC,GAAR,CAAY,MAAZ;;AACA,cAAA,MAAI,CAAC,YAAL,CAAkB,IAAlB,CAAuB,MAAvB;AACJ;AACH,WAND;AAOH,SATD;AAWJ,OAzBA,MAyBO;AACH,QAAA,MAAI,CAAC,IAAL,GAAY,IAAZ;AACJ;AACH,KA7BD;AA8BJ;AAhGW,CAAf","sourcesContent":["<template>\n<div class = \"page\">\n    <link rel=\"stylesheet\" \n        href=\"https://use.fontawesome.com/releases/v5.2.0/css/all.css\" \n        integrity=\"sha384-hWVjflwFxL6sNzntih27bfxkr27PmbbK/iSvJ+a4+0owXq79v+lsFkW54bOGbiDQ\" \n        crossorigin=\"anonymous\">\n    <Navbar/>\n    <div class=\"container\" id=\"main-container\">\n        <h1 style=\"color: black\" class=\"mb-4 header\">Profile Page</h1>\n         <div class=\"container-fluid justify-content-center\">\n            <div class=\"row\">\n                <ul class=\"nav nav-tabs\" id=\"myTab\" role=\"tablist\">\n                    <li class=\"nav-item mx-3\" role=\"presentation\">\n                        <a class=\"nav-link active\" id=\"home-tab\" data-bs-toggle=\"tab\" href=\"#about\" role=\"tab\" aria-controls=\"about\" aria-selected=\"true\">About Me</a>\n                    </li>\n                    <li class=\"nav-item mx-3\" role=\"presentation\">\n                        <a class=\"nav-link\" id=\"tasks-tab\" data-bs-toggle=\"tab\" href=\"#tasks\" role=\"tab\" aria-controls=\"tasks\" aria-selected=\"false\">My Tasks</a>\n                    </li>\n                    <li class=\"nav-item mx-3\" role=\"presentation\">\n                        <a class=\"nav-link\" id=\"reviews-tab\" data-bs-toggle=\"tab\" href=\"#reviews\" role=\"tab\" aria-controls=\"reviews\" aria-selected=\"false\">Reviews</a>\n                    </li>\n                    <!-- <li class=\"nav-item mx-3\" role=\"presentation\">\n                        <a class=\"nav-link\" id=\"settings-tab\" data-bs-toggle=\"tab\" href=\"#settings\" role=\"tab\" aria-controls=\"settings\" aria-selected=\"false\">Settings</a>\n                    </li> -->\n                </ul>\n                <div class=\"tab-content\" id=\"myTabContent\">\n                    <div class=\"tab-pane fade show active\" id=\"about\" role=\"tabpanel\" aria-labelledby=\"about-tab\">\n                        <div class=\"container-fluid justify-content-center\">\n                            <div class=\"row justify-content-center\" v-for=\"(details,index) in user_details\" v-bind:key=\"index\">\n                                <div class=\"col-sm-3 mt-4 mb-4 imagepos\">\n                                    <img :src=\"details.image\" id=\"image\" class=\"img-fluid rounded float-start border-2 border-dark\" alt=\"...\">\n                                </div>\n                                <div class=\"card col-sm-9 mx-2 mt-4\">\n                                    <ul class=\"list-group list-group-flush\" >\n                                        <li class=\"list-group-item\">\n                                            <div class=\"my-1 row mx-3\">\n                                                <label class=\"col-sm-4 col-form-label\"><b>Name:</b></label>\n                                                <div class=\"col-sm-8 details\">{{details.name}}</div>\n                                            </div>\n                                        </li>\n                                        <li class=\"list-group-item bg-light\">\n                                            <div class=\"my-1 row mx-3 \">\n                                                <label class=\"col-sm-4 col-form-label\"><b>Department:</b></label>\n                                                <div class=\"col-sm-8 details\">{{details.department_id}}</div>\n                                            </div>\n                                        </li>\n                                        <li class=\"list-group-item\">\n                                            <div class=\"my-1 row mx-3\">\n                                                <label class=\"col-sm-4 col-form-label\"><b>Position:</b></label>\n                                                <div class=\"col-sm-8 details\">{{details.position}}</div>\n                                            </div>\n                                        </li>\n                                        <li class=\"list-group-item bg-light\">\n                                            <div class=\"my-1 row mx-3\">\n                                                <label class=\"col-sm-4 col-form-label\"><b>Employment Type:</b></label>\n                                                <div class=\"col-sm-8 details\">{{details.employment_type}}</div>\n                                            </div>\n                                        </li>\n                                        <li class=\"list-group-item\">\n                                            <div class=\"my-1 row mx-3\">\n                                                <label class=\"col-sm-4 col-form-label\"><b>Email:</b></label>\n                                                <div class=\"col-sm-8 details\">{{details.email}}</div>\n                                            </div>\n                                        </li>\n                                        <li class=\"list-group-item bg-light\">\n                                            <div class=\"my-1 row mx-3\">\n                                                <label class=\"col-sm-4 col-form-label\"><b>Password:</b></label>\n                                                <div class=\"col-sm-8 details\">{{details.password}}</div>\n                                            </div>\n                                        </li>\n                                        <li class=\"list-group-item\">\n                                            <div class=\"my-1 row mx-3\">\n                                                <label class=\"col-sm-4 col-form-label\"><b>Current Points:</b></label>\n                                                <div class=\"col-sm-8 details\">{{details.current_pts}}</div>\n                                            </div>\n                                        </li>\n                                    </ul>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"tab-pane fade\" id=\"tasks\" role=\"tabpanel\" aria-labelledby=\"tasks-tab\">\n                    <input type=\"text\" placeholder=\" Filter by task/status\" class=\"w-100 mt-3\" v-model=\"filter\" />\n                    <table class=\"table table-hover mt-3\">\n                        <thead>\n                            <tr>\n                                <th style=\"background: #b6cefb\" class=\"text-center\" scope=\"col\">Task Status <button class=\"button\" @click=\"sortTable('status', direction)\"><img class=\"sort\" src=\"../assets/sort.png\"></button></th>\n                                <th style=\"background: #b6cefb\" class=\"text-center\" scope=\"col\">Task Name <button class=\"button\" @click=\"sortTable('task', direction)\"><img class=\"sort\" src=\"../assets/sort.png\"></button></th>\n                                <th style=\"background: #b6cefb\" class=\"text-center\" scope=\"col\">Project Name <button class=\"button\" @click=\"sortTable('proj', direction)\"><img class=\"sort\" src=\"../assets/sort.png\"></button></th>\n                            </tr>\n                        </thead>\n                        <tbody>\n                            <tr v-for=\"(row, index) in filteredRows\" :key=\"`task-${index}`\">\n                                <td class=\"text-center\">{{ row.task_status }}</td>\n                                <td class=\"text-center\">{{ row.task_name }}</td>\n                                <td class=\"text-center\">{{ row.project_name }}</td>\n                            </tr>\n                        </tbody>\n                    </table>\n                </div>\n                <div class=\"tab-pane fade\" id=\"reviews\" role=\"tabpanel\" aria-labelledby=\"reviews-tab\">\n                    <div class=\"height-100 container mt-3\">\n                        <div class=\"card p-3\">\n                            <div class=\"d-flex justify-content-between align-items-center\">\n                                <div class=\"ratings\"> \n                                    {{averageReviews}}\n                                    <vue3StarRatings v-model=\"averageReviews\" v-bind:showControl = null />\n                                    <!-- <i class=\"fa fa-star rating-color\" v-for=\"index in averageReviews\" :key=\"index\"  ></i> \n                                \n                                    <i class=\"fa fa-star\" v-for=\"index in 5-averageReviews\" :key=\"index\" ></i>  -->\n                                </div>\n                                <h5 class=\"review-count\"> {{user_reviews.length}} Reviews</h5>\n                            </div>\n                            <div class=\"mt-5 card\">\n                                <div class=\"card-body d-flex flex-column align-items-center\" v-for =\"(review, index) in user_reviews\" v-bind:key=\"index\">\n                                    <h5 class=\"review-stat card-title\">Anon</h5>\n                                    <!-- {{review}} -->\n                                    <div class=\"small-ratings card-subtitle\"> \n                                        <vue3StarRatings v-model=\"review.rating\" v-bind:showControl = null />\n                                        <i v-for=\"index in review.rating\" :key=\"index\" class=\"fa fa-star rating-color\"></i> \n                                        <i v-for=\"index in 5-review.rating\" :key=\"index\"  class=\"fa fa-star\"></i> \n                                    </div>\n                                    <p class=\"card-text\">{{review.comments}} </p>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"tab-pane fade\" id=\"settings\" role=\"tabpanel\" aria-labelledby=\"settings-tab\">...</div>\n                </div>\n                <!-- <div class=\"tab-pane fade\" id=\"settings\" role=\"tabpanel\" aria-labelledby=\"settings-tab\">...</div> -->\n            </div>\n        </div>\n    </div>\n    </div>\n</div>    \n</template>\n\n<script>\nimport vue3StarRatings from \"vue3-star-ratings\";\nimport Navbar from \"./Navbar.vue\"\nimport mixin from \"../mixin\"\nimport firebase from \"firebase/compat\"\nexport default {\n    name:\"ProfilePage\",\n    components: {\n        Navbar,\n        vue3StarRatings\n    },\n    mixins:[mixin],\n    methods: {\n        sortTable(key,direction) {\n            this.sort = `${key} > ${direction}`\n            if (direction === 'asc') {\n                this.rows.sort((a, b) => a[key] > b[key] ? 1: -1)\n                this.direction = 'desc';\n            } else {\n                this.rows.sort((a, b) => a[key] < b[key] ? 1: -1)\n                this.direction = 'asc';\n            }\n        }\n    },\n      beforeMount(){\n    //    this.usertype = this.getUserType()\n    //    console.log(this.usertype)\n    //     if(this.getUserType() != \"employee\"){\n    //         this.$router.push(\"/\")\n    //     }\n  },\n    data() {\n        return {\n            user_details: [],\n            user_tasks: [],\n            user_reviews: [],\n            filter: '',\n            sort: '',\n            direction: 'asc',\n            columns: ['task','proj','status','person'],\n            // rows: [\n            //     {task:'Setup Database', proj: 'Project 2', status: 'In Progress', person: 'John', deadline: '20/11/2021'},\n            //     {task:'Draft Proposal', proj: 'Project 1', status: 'New', person: 'John', deadline: '2/11/2021'},\n            //     {task:'UI Testing', proj: 'Project 1',status: 'Completed', person: 'John', deadline: '10/11/2021'}\n            // ],\n            // details: {name:\"John\", password:\"IT123john\", email:\"john_IT.gmail.com\", department:\"IT\", position:\"Team Lead\", current_points:\"10\", employment_type: \"Full Time\"}\n        }\n    },\n    computed: {\n        filteredRows() {\n                return this.user_tasks.filter(row => {\n                const task = row.task_name.toLowerCase();\n                const status = row.task_status.toLowerCase();\n                const project = row.project_name.toLowerCase();\n                const searchTerm = this.filter.toLowerCase();\n\n                return task.includes(searchTerm) || status.includes(searchTerm) || project.includes(searchTerm);\n            });\n        },\n        averageReviews(){\n            var total = 0\n            var listedRatings = this.user_reviews;\n            for(let i = 0; i < listedRatings.length; i++){\n                // console.log(listedRatings[i].ratings)\n                total+= listedRatings[i].rating\n            }\n\n            return total/listedRatings.length\n        }\n    },\n    created() {\n        firebase.auth().onAuthStateChanged((user) => {\n            if (user) {\n                firebase.database().ref('users/' + user.uid ).on('value', (snapshot) => {\n                    this.user_details.push(snapshot.val()); \n                    console.log(this.user_details) \n                }); \n                firebase.database().ref('tasks/').on('value', (snapshot) => {\n                    snapshot.forEach((childSnapshot) => {\n                        var task = childSnapshot.val();\n                        if (task.user_id === user.uid) {\n                            console.log(task)\n                            this.user_tasks.push(task)\n                        } \n                    });\n                }); \n                firebase.database().ref('reviews/').on('value', (snapshot) => {\n                    this.user_reviews = []\n                    snapshot.forEach((childSnapshot) => {\n                        var review = childSnapshot.val();\n                        if (review.user_id === user.uid) {\n                            console.log(review)\n                            this.user_reviews.push(review)\n                        } \n                    });\n                }); \n\n            } else {\n                this.user = null;\n            }\n        }); \n    }\n}\n\n</script>\n\n<style>\n.card{\n    /* width: auto; */\n    height: auto;\n}\n#main-container{\n    width: auto;\n    height: auto;\n}\n.imagepos{\n    width: 270px;\n    height: 200px;\n}\n#image{\n    max-width: 100%;\n    height: auto;\n}\nh1{\n    margin-top: 80px;\n}\n.page{\n    min-height: 100vh;\n    width:100%;\n    background: white;\n    /* display: flex; */\n}\n.pageContent{\n  min-height:100vh;\n  /* margin-top:60px; */\n  background:rgb(248, 248, 248);\n  padding-top:60px;\n  padding-bottom:60px;\n}\n</style>\n\n<style scoped>\nh3 {\n    margin-top: 80px;\n    margin-left: 32px;\n    text-align: left;\n}\nlabel {\n    text-align: left;\n}\n.details {\n    text-align: left;\n    padding: 7px 12px 7px 12px;\n}\n.sort {\n    width: 20px;\n}\n.button {\n    border: none;\n    background-color: transparent;\n}\n.ratings {\n    margin-right: 10px\n}\n.ratings i {\n    color: #cecece;\n    font-size: 32px\n}\n.rating-color {\n    color: #fbc634 !important\n}\n.review-count {\n    font-weight: 400;\n    margin-bottom: 2px;\n    font-size: 24px !important\n}\n.small-ratings i {\n    color: #cecece\n}\n.review-stat {\n    font-weight: 300;\n    font-size: 18px;\n    margin-bottom: 2px;\n    text-align: left;\n}\n\n.header{\n    box-shadow: 0px 5px 0px rgba(83, 90, 249, 0.81);\n    width: max-content;\n    margin: 80px auto;\n} \n\n</style>"],"sourceRoot":""}]}