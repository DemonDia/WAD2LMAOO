a class=\"textarea\" role=\"textbox\"></textarea></td>\r\n                </tr>\r\n\r\n                <tr>\r\n                    <th><label>Additional Comments: </label></th>\r\n                    <td><textarea class=\"textarea\" role=\"textbox\"></textarea></td>\r\n                </tr>      \r\n\r\n                <tr>\r\n                    <th><label>Overall Rating: </label></th>\r\n                    <td>\r\n                        <vue3StarRatings v-model=\"rating\" v-bind:showControl = null />\r\n                        \r\n                    </td>\r\n                </tr>            \r\n            </table>\r\n             {{rating}}\r\n             <div class = \"btn-container\">\r\n                 <router-link class = 'btn btn-danger' to=\"/review\">\r\n                 Cancel\r\n                 </router-link>\r\n                 <!-- <button class=  'btn btn-danger'>Cancel</button> -->\r\n                 <button class=  'btn btn-primary'\r\n                 style = \"background:#504DFF\"\r\n                 v-on:click = \"submit\">Submit</button>\r\n             </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\n// import Rating from 'v-rating';\r\nimport vue3StarRatings from \"vue3-star-ratings\";\r\nimport mixin from \"../mixin\"\r\n// import Navbar from \"./Navbar.vue\"\r\nexport default {\r\n    name:\"MemberToReview\",\r\n    components:{\r\n        // Navbar,\r\n        vue3StarRatings\r\n    },\r\n    mixins:[mixin],\r\n    data(){\r\n        return{\r\n            rating:0\r\n        }\r\n    },\r\n      beforeMount(){\r\n       this.getUserType()\r\n        if(this.usertype !== \"employer\"){\r\n            this.$router.push(\"/\")\r\n        }\r\n  },\r\n\r\n    methods:{\r\n        submit(){\r\n            alert(\"You have submitted review for\" + this.accountList.name); // need get the name of user\r\n            this.$router.push(\"/review\")\r\n        }\r\n    }\r\n\r\n}\r\n</script>\r\n<style scoped>\r\n\r\n .page{\r\n        min-height: 100vh;\r\n        width:100%;\r\n        background:rgba(248, 248, 248, 1);\r\n        /* display: flex; */\r\n    }\r\n.review-container{\r\n    /* background:red; */\r\n    margin-top:80px;\r\n    display:grid;\r\n    justify-items: center;\r\n}\r\nh2{\r\n    margin:10px;\r\n}\r\n/* member card */\r\n.member-card{\r\n    width:300px;\r\n    border-radius:10px;\r\n}\r\n.person-image{\r\n    width:100%;\r\n    height:300px;\r\n    background: #EFEFEF;\r\n\r\n}\r\n.member-name{\r\n    padding:10px;\r\n    background: #6B8AFA;\r\n    border-radius: 10px 10px 0px 0px;\r\n    margin:0px;\r\n    color:white;\r\n}\r\n\r\n/* review form */\r\n.review-form{\r\n    margin:10px;\r\n    /* background:orange; */\r\n    /* height:1000px; */\r\n    width:80%;\r\n    padding:20px;\r\n}\r\n.form-row{\r\n    display: flex;\r\n    align-items: center;\r\n    width:100%;\r\n    justify-content: center;\r\n    margin:10px;\r\n}\r\ntextarea{\r\n    resize: both;\r\n}\r\nth{\r\n    text-align: end;\r\n    width:40%;\r\n}\r\nth label{\r\n    margin:10px;\r\n}\r\ntd{\r\n    text-align: start;\r\n}\r\ntd textarea{\r\n    width: 60%;\r\n    /* margin:10px; */\r\n}\r\n/* .form-row textarea, .form-row label{\r\n    margin:10px;\r\n} */\r\n/* rating */\r\n.vue3-star-ratings__wrapper{\r\npadding: 0;\r\n    margin: 0;\r\n    width: max-content;    \r\n}\r\n\r\n/* for button */\r\n.btn-container{\r\n    margin:10px;\r\n    width:80%;\r\n    /* background: black; */\r\n    display: flex;\r\n    justify-content: space-evenly;\r\n}\r\n</style>"],"sourceRoot":""}]}